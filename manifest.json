{
  "manifest_version": 3,
  "name": "Swift Wipe",
  "description": "Swift Cookie Wipe helps you to delete cookies, session store for the actual website.",
  "version": "1.0",
  "permissions": ["cookies", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
